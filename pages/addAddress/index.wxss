/* pages/addAddress/index.wxss */
input{
  border: 1px solid #000;
  height: 60rpx;
}
.address{
  width: 100rpx;
  height: 100rpx;
  border: 1rpx solid #000;
}
.province{
  position: absolute;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  background: #fff;
  overflow: auto;
  z-index: 40
}
.city{
  position: absolute;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  background: #fff;
  overflow: auto;
  z-index: 50
}
.area{
  position: absolute;
  bottom: 0;
  top: 0;
  left: 0;
  right: 0;
  background: #fff;
  overflow: auto;
  z-index: 60
}
.select{
  display: block;
  height: 60rpx;
  border: 1px solid #000;
}
